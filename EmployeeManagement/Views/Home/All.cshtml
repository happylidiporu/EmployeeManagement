@model IEnumerable<EmployeeManagement.Models.Employee>

@{
    ViewData["Title"] = "All";
}

<h2>All</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Name
            </th>
            <th>
                Email
            </th>
            <th>
                Description
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.Id)
            </td>
            <td>
                @item.Name)
            </td>
            <td>
                @item.Email)
            </td>
            <td>
                @item.Departement)
            </td>
        </tr>
}
    </tbody>
</table>
